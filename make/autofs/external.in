config EXTERNAL_FREETZ_PACKAGE_AUTOFS
	depends on EXTERNAL_ENABLED && FREETZ_PACKAGE_AUTOFS
	bool "autofs"
	default n
	help
		externals the following file(s):
		 /usr/bin/automount
		 /usr/lib/freetz/autofs/lookup_file.so
		 /usr/lib/freetz/autofs/lookup_hosts.so
		 /usr/lib/freetz/autofs/lookup_multi.so
		 /usr/lib/freetz/autofs/lookup_program.so
		 /usr/lib/freetz/autofs/lookup_userhome.so
		 /usr/lib/freetz/autofs/mount_afs.so
		 /usr/lib/freetz/autofs/mount_autofs.so
		 /usr/lib/freetz/autofs/mount_bind.so
		 /usr/lib/freetz/autofs/mount_changer.so
		 /usr/lib/freetz/autofs/mount_generic.so
		 /usr/lib/freetz/autofs/mount_nfs.so
		 /usr/lib/freetz/autofs/parse_sun.so
